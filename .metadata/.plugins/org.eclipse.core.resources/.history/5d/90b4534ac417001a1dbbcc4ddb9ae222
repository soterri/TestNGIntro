package com.class30;

public class VehicleTest {

	public static void main(String[] args) {
		
		//new BMW(); gets a CE bc you need to pass a value
		BMW bmw=new BMW("767JBH676", "Sedan", "BMW", "X5");
		//comes from VEHICLE class	
		bmw.drive();
		bmw.stop();
		bmw.speed();
		bmw.start();
		//comes from CAR class
		bmw.breaking();
		//comes from BMW
		bmw.display();
		
		Car car=new BMW("767JBH676", "Sedan", "BMW", "X5");
		car.drive();
		car.stop();
		car.speed();
		car.start();
		//comes from car class
		bmw.breaking();
		//comes BMW - wont be available to the parent
		//car.display();
		
		Vehicle vehicle=new BMW("767JBH676", "Sedan", "BMW", "X5");
		vehicle.drive();
		vehicle.stop();
		vehicle.speed();
		vehicle.start();
		//comes from car class -- wont be avail to the parent class
		//vehicle.breaking();
		//comes from BMW class -- wont be avail to the grandparent class 
		//vehicle.display();
		
	}
	

}
