package com.class27;

public class Student {

	public void timeOfClass() {
		System.out.println("Class starts at 8 in the morning");
		}
	public void dayOfClass() {
		System.out.println("The day of the week of class starting is Monday");
	}
	
		}
class syntaxStudent extends Student() {
	public void timeOfClass() {
	System.out.println("I am a student in Syntax");
	}
}

class collegeStudent extends Student() {
	public void timeOfClass() {
	System.out.println("I am a college student");
}
}
class schoolStudent extends Student() {
	public void timeOfClass() {
	System.out.println("My kids are school students");
}
}


