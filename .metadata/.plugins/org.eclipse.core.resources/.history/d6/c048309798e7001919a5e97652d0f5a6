package com.class5;

public class LogicalNot {

	public static void main(String[] args) {
		
		
		
		
		boolean b=!true;
		boolean b2=!false;
		
		System.out.println(b);
		System.out.println(b2);
		
		boolean traffic=true;
		
		if(!traffic) {
			System.out.println("Hello");
		}else {
			System.out.println("Bye");
		}
		
		

	}

}
