package com.repltasks;

public class NestedForLoops071 {

	public static void main(String[] args) {
	
		
		for(int i=1; i<11; i++) {
			System.out.print(+i+" ");
		}
		System.out.println();
	
			for(int j=1; j<=20; j++) {
				if(j%2==0) {
					System.out.print(+j+" ");
				}
				System.out.println();
		}
			for(int k=3; k<=30; k+=3) {
				System.out.print(+k+" ");
			}
		System.out.println();

	}

	




	}

}
