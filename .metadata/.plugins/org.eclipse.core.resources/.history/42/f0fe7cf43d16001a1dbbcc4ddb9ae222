package com.class29;

public abstract class File {

	public void JavaFile() {
		System.out.println("This is to open");
	}
	public void WordFile() {
		System.out.println("This is to edit");
	}
	public void PDFFile() {
		System.out.println("This is to close");
	}
	public abstract void toOpen();
	
	public abstract void toEdit();
	
	public abstract void toClose();
}
//CONCRETE CLASS
class JavaFile extends File{

	@Override
	public void toOpen() {
		System.out.println("To open java.file");
		
	}

	@Override
	public void toEdit() {
		System.out.println("To edit a java.file");
		
	}

	@Override
	public void toClose() {
		System.out.println("To close a java.file");
		
	}
	
}

