		  boolean bothEven(int num1, int num2){
		    boolean isEven;
		    
				if(num1%2==0) {
					isEven=true;
				}else {
					isEven=false;
				}
				return isEven;
		    
		  }
		  public static void main(String[] args){
		    Main obj=new Main();
		    boolean result=obj.bothEven(3,4);
		    System.out.println(result);
		  
